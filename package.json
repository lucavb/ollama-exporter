{
    "name": "ollama-exporter",
    "version": "1.0.0",
    "description": "A TypeScript/Node.js Prometheus exporter that collects and exposes metrics from Ollama API for monitoring and observability",
    "main": "src/index.ts",
    "type": "module",
    "scripts": {
        "build": "tsc --project tsconfig.build.json",
        "start": "node --experimental-strip-types src/index.ts",
        "start:dist": "node dist/index.js",
        "dev": "node --experimental-strip-types src/index.ts",
        "validate-config": "node --experimental-strip-types src/index.ts --validate-config",
        "cq": "npm run cq:eslint && npm run cq:format:check && npm run cq:typecheck",
        "cq:eslint": "eslint src",
        "cq:format": "prettier --write .",
        "cq:format:check": "prettier --check .",
        "cq:typecheck": "tsc --noEmit --project tsconfig.spec.json",
        "cq:test": "vitest run",
        "test": "vitest run",
        "semantic-release": "semantic-release"
    },
    "keywords": [
        "ollama",
        "prometheus",
        "exporter",
        "metrics",
        "monitoring",
        "observability",
        "typescript",
        "nodejs",
        "llm",
        "ai",
        "large-language-model",
        "telemetry",
        "grafana",
        "docker"
    ],
    "author": "Luca Becker <hello@luca-becker.me>",
    "license": "GPL-3.0-only",
    "homepage": "https://luca-becker.me",
    "repository": {
        "type": "git",
        "url": "https://github.com/lucavb/ollama-exporter.git"
    },
    "bugs": {
        "url": "https://github.com/lucavb/ollama-exporter/issues"
    },
    "engines": {
        "node": "v25.2.1"
    },
    "dependencies": {
        "@commander-js/extra-typings": "14.0.0",
        "prom-client": "15.1.3"
    },
    "devDependencies": {
        "@eslint/js": "9.39.2",
        "@semantic-release/exec": "7.1.0",
        "@semantic-release/github": "11.0.6",
        "@semantic-release/npm": "12.0.2",
        "@types/node": "24.10.4",
        "conventional-changelog-conventionalcommits": "9.1.0",
        "eslint": "9.39.2",
        "jiti": "2.6.1",
        "prettier": "3.7.4",
        "semantic-release": "24.2.9",
        "tsx": "4.21.0",
        "typescript": "5.9.3",
        "typescript-eslint": "8.50.1",
        "vitest": "3.2.4"
    }
}
